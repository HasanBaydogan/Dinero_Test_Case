{
  "rewrites": [
    {
      "source": "/api/provinces",
      "destination": "https://api.aidath.com/api/v1/global/public/getprovincesordistricts"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization, client-id, os-id"
        }
      ]
    }
  ],
  "functions": {
    "api/upload-cv.js": {
      "maxDuration": 30
    }
  }
} 